    "version": "0.2.0",
    "configurations": [   
        {
            "name": "Python Debugger: Django Tests",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "test", "flowerapp"
            ],
            "justMyCode": false,
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\python\\flower\\manage.py"
        },
        {
            "name": "Python Django runserver",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "justMyCode": false,
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\python\\flower\\manage.py"
        },
        {
            "name": "tag tests",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "test", "flowerapp.test_track_template"
            ],
            "justMyCode": false,
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\python\\flower\\manage.py"
        }


        
    ]
}